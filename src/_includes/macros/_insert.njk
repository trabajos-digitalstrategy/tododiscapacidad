</div>
{% else %}
<div class="word {{type}}"    {% if animationDelay %}  style="animation-delay: {{ animationDelay }}ms "   {% endif %} >{{ palabra }}</div>
{% endif %}
{% set animationDelay = animationDelay + animationTime %}
{% endfor%}
{% endmacro %}



