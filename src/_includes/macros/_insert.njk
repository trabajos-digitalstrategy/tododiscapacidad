{% macro splitenpalabras(frase = '', type = '', animationTime='', animationDelay='' ) %}
{% set palabras = frase.split(" ") %}
<div class="word-wrap">
  {% for palabra in palabras %}
  {% if palabra.startsWith('**') %}
  <span class="word c-accent {{type}} "  {% if animationDelay %} style="animation-delay: {{ animationDelay }}ms "   {% endif %} >{{ palabra | replace('**', '') }}
  </span>
  {% elseif palabra == '--' %}
  {{ palabra | replace('--', '<br>') | safe }}
  {% else %}
  <span class="word {{type}}"   {% if animationDelay %} style="animation-delay: {{ animationDelay }}ms "   {% endif %} >{{ palabra }}</span>
  {% endif %}
  {% set animationDelay = animationDelay + animationTime %}
  {% endfor%}
</div>
{% endmacro %}


{% macro splitenlistas(frase = '', type = '', animationTime='', animationDelay='' ) %}
